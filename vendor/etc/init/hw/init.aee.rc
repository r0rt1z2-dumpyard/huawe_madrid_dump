on init
    setprop ro.vendor.aee.enforcing no

on post-fs-data
    mkdir /data/vendor/log 0775 root root
    mkdir /data/vendor/log/mtklog 0775 root root
    mkdir /data/vendor/log/mtklog/aee_exp 0775 root root
    mkdir /data/vendor/dumpsys 0775 system system
    mkdir /data/vendor/core 0775 system system
    mkdir /data/vendor/tombstones 0775 system system

on post-fs-data
    start aee_aedv
    start aee_aedv64
